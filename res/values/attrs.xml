<?xml version="1.0" encoding="utf-8"?>
<resources>

    <declare-styleable name="Gallery">
        <attr name="android:galleryItemBackground" />
    </declare-styleable>
    <declare-styleable name="SkyGallery">
        <attr name="animationDuration" format="integer" />
        <attr name="space" format="dimension" />
        <attr name="circle" format="boolean" />
        <attr name="slot" format="boolean" />
    </declare-styleable>
    <declare-styleable name="sky">
        <attr name="circlable" format="boolean" />
        <attr name="scrollable" format="boolean" />
        <attr name="bouncable" format="boolean" />
        <attr name="federnSpace" format="dimension" />
        <attr name="text" format="string" />
        <attr name="src" format="reference" />
        <attr name="color" format="reference|color" />
        <attr name="textSize" format="reference|dimension" />
    </declare-styleable>

</resources>